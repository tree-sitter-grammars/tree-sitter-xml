{
  "name": "@tree-sitter-grammars/tree-sitter-xml",
  "version": "0.6.2",
  "license": "MIT",
  "description": "XML & DTD grammars for tree-sitter",
  "repository": "tree-sitter-grammars/tree-sitter-xml",
  "main": "bindings/node",
  "types": "bindings/node",
  "author": {
    "name": "ObserverOfTime",
    "email": "chronobserver@disroot.org",
    "url": "https://observeroftime.github.io/"
  },
  "contributors": [
    {
      "name": "Amaan Qureshi",
      "email": "amaanq12@gmail.com"
    }
  ],
  "funding": {
    "url": "https://github.com/sponsors/ObserverOfTime",
    "type": "github"
  },
  "keywords": [
    "incremental",
    "parsing",
    "tree-sitter",
    "xml",
    "dtd"
  ],
  "files": [
    "binding.gyp",
    "prebuilds/**",
    "bindings/node/*",
    "queries/**",
    "xml/grammar.js",
    "dtd/grammar.js",
    "xml/package.json",
    "dtd/package.json",
    "xml/src/**",
    "dtd/src/**",
    "common/**"
  ],
  "dependencies": {
    "node-addon-api": "^8.0.0",
    "node-gyp-build": "^4.8.0"
  },
  "devDependencies": {
    "prebuildify": "^6.0.0",
    "tree-sitter-cli": "^0.22.2"
  },
  "peerDependencies": {
    "tree-sitter": "^0.21.0"
  },
  "peerDependenciesMeta": {
    "tree_sitter": {
      "optional": true
    }
  },
  "overrides": {
    "tree-sitter": {
      "node-addon-api": "$node-addon-api"
    }
  },
  "scripts": {
    "install": "node-gyp-build",
    "build": "prebuildify --napi --strip",
    "version": "make update"
  },
  "publishConfig": {
    "access": "public"
  },
  "tree-sitter": [
    {
      "scope": "source.xml",
      "file-types": [
        "xml",
        "svg",
        "xsd",
        "xslt",
        "xsl",
        "rng"
      ],
      "injection-regex": "^(xml|svg|xsd|xslt|xsl|rng)$",
      "highlights": "queries/xml/highlights.scm",
      "path": "xml",
      "external-files": [
        "common/scanner.h"
      ]
    },
    {
      "scope": "source.dtd",
      "file-types": [
        "dtd"
      ],
      "injection-regex": "^dtd$",
      "highlights": "queries/dtd/highlights.scm",
      "path": "dtd",
      "external-files": [
        "common/scanner.h"
      ]
    }
  ]
}
